<UserControl x:Class="DiplomWPF.ProcessControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <GroupBox Header="Process Name" Height="59" HorizontalAlignment="Left" Margin="0,0,0,241" Name="processGroupBox" VerticalAlignment="Bottom" Width="155" BorderBrush="{x:Null}">
            <Grid>
                <CheckBox Content="" Height="16" HorizontalAlignment="Left" Margin="7,9,0,0" Name="selectProcess" VerticalAlignment="Top" />
                <Button Content="Выбрать" Height="23" HorizontalAlignment="Left" Margin="31,6,0,0" Name="setProcessButton" VerticalAlignment="Top" Width="107" />
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
